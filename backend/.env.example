DATABASE_URL=postgresql+psycopg://quieteye:quieteye@localhost:5432/quieteye

# Application Environment
# Options: dev, staging, production
QUIETEYE_ENV=dev

# Server Configuration
# Host to bind to (0.0.0.0 for all interfaces)
# PORT=8000

